@model  SmartCity.Models.TabularModel_Extended
<style>
    select, textarea, input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input
    {
        margin-bottom: 0;
        font-size:12px;
    }
</style>
<div class="row-fluid">
    <div class="span24">
        @Html.Label("Date Range:", new { @class = "pagination-right" })
        @Html.EditorFor(model => model.searchParam.FromDate, new { @class = "date datepicker" })
        @Html.EditorFor(model => model.searchParam.ToDate, new { @class = " date datepicker" })
        @Html.DropDownList("ChartOption", new SelectList(Model.searchParam.ddlChartOption, "Key", "Value"))
        @Html.DropDownList("WeatherParameter", new SelectList(Model.searchParam.ddlWeatherParameter, "Key", "Value"), new { @class="hidden"})
    </div>
</div>
<div class="row-fluid">
    <div class="span24">
        @Html.Label("Select Station:", new { @class = "pagination-right" })
        @Html.DropDownList("DisplayOption", new SelectList(Model.searchParam.ddlMeasureOption, "Key", "Value"))
        @Html.DropDownList("Station", new SelectList(Model.searchParam.ddlStation, "Key", "Value"))
        @Html.TextBoxFor(model => model.searchParam.StationList, new { @readonly="readonly"})
        <input type="button" id="Search" value="Search" class="btn" />
    </div>
</div>


